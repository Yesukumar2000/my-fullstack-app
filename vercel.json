{
    "version": 2,
        "builds": [
          {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "build"
            }
          }
        ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ],
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  }
  